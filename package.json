{
  "name": "nuxt-jest-storybook-ts-cypress-vuetify",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore . --ignore-pattern \"__cypress__/support/index.d.ts\"",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore . ",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "prepare": "husky install",
    "storybook": "start-storybook -s ./public -p 6006 -c .storybook",
    "storybook:build": "build-storybook",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "commitizen": "4.2.4",
    "core-js": "^3.19.3",
    "css-loader": "4.2.0",
    "cz-conventional-changelog": "3.3.0",
    "dinero.js": "^1.9.1",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.8",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.6.1",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-typescript": "^7.17.12",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-storysource": "^6.5.5",
    "@storybook/addon-viewport": "^6.5.5",
    "@storybook/vue": "6.4.19",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/vue": "5",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.36",
    "@types/prettier": "^2.6.3",
    "@vue/server-test-utils": "1",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.4.4",
    "babel-loader": "^8.2.5",
    "babel-preset-vue": "^2.0.2",
    "cypress": "^9.6.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "27.1.1",
    "lint-staged": "^12.1.2",
    "material-design-icons-iconfont": "^6.7.0",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "rtl-sass": "1.5.0",
    "sass": "1.32.6",
    "sass-loader": "10.1.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "ts-jest": "^27.1.1",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
